{
  "name": "github_api",
  "version": "1.0.0",
  "description": "Example project to prove skills",
  "main": "index.js",
  "repository": "git@github.com:lwrooblewski/github_api.git",
  "author": "lwrooblewski <l.wrooblewski@gmail.com>",
  "license": "MIT",
  "engines": {
    "yarn": "1.22.4",
    "node": ">=12.14.0"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack-dev-server --config webpack.config.js",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx",
    "prettify": "prettier --write src/*",
    "pretty-quick": "pretty-quick --pattern './src/*' --staged",
    "husky-pre-commit": "yarn run pretty-quick && staged && lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "yarn run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run husky-pre-commit"
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.20",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
